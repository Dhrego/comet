  }

  .hobby-title svg {
    transform: rotate(-90deg);
    transition: transform 0.5s ease;
  }

  .hobby-title.active {
    color: var(--white);
  }

  .hobby-title.active svg {
    transform: rotate(0);
  }

  .hobby-list {
